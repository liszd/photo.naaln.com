<script src="//cdn.bootcss.com/jquery/2.2.4/jquery.min.js"></script>
<script src="//cdn.bootcss.com/video.js/5.11.0/video.min.js"></script>
<script src="//cdn.bootcss.com/responsive-nav.js/1.0.39/responsive-nav.min.js"></script>

<%- partial('third-party/instagram') %>
<%- partial('third-party/motto') %>
<%- partial('third-party/lazyload') %>
<%- partial('third-party/highlight') %>
<%- partial('third-party/analytics') %>

<%if (is_archive()==true) { %>
    <script type="text/javascript">
    $(document).ready(function(e) {
      var h = $(".main ul li:first-child").height();
      var h1 = $(".main ul li:last-child").height();
      $(".line").css("top",h);
      $(".line").height($(".main").height()-h1-h);
    });
    </script>
<% } %>

<script>
    var nav = responsiveNav(".site-nav",{
        label: '<i class="fa fa-bars"></i>',
        transition: 400,
        open: function(){
            $('#main').css('margin-top','40px');
        },
        close: function(){
            $('#main').css('margin-top','-70px');
        },
    });
</script>

